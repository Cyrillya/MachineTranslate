Commands: {
	TranslateCommand: {
		Description: 翻译给予的内容并将其添加到剪贴板中。
		Usage:
			'''
			/trans <内容>
			    内容 - 要翻译的文本。
			    注意：必须指定语言代码，而不是语言名称。如“en”“zh”“ru”等。
			'''
	}

	WebsiteCommand: {
		Description: 打开你在配置选项中选择的翻译网站。
		Usage: /tweb
	}
}